    "version": "0.2.0",
    "configurations": [
        { 
            "name":"QUBO Solver",
            "type":"python",
            "request":"launch",
            "program":"python/qubo_solver/src/main.py",
            "args": [".vrp/att48_0.coo", "sim"],
            "console":"integratedTerminal"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'pipeline'",
            "cargo": {
                "args": [
                    "+nightly",
                    "build",
                    "--bin=pipeline",
                    "--package=pipeline",
                    "--all-features"
                ],
                "filter": {
                    "name": "pipeline",
                    "kind": "bin"
                }
            },
            "args": ["solve", "src/res/gil262.vrp"],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'pipeline'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=pipeline",
                    "--package=pipeline"
                ],
                "filter": {
                    "name": "pipeline",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}